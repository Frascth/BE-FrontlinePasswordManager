{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [0, "ignorePackages"],
        "no-console": "off",
        "padded-blocks": "off",
        "class-methods-use-this": "off",
        "object-curly-newline": "off",
        "no-param-reassign": "off",
        "no-await-in-loop": "off"
    }
}
